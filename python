sudo dnf install -y python3 python3-pip python3-devel gcc make \
                   zlib-devel bzip2 bzip2-devel xz-devel \
                   libffi-devel sqlite sqlite-devel \
                   readline-devel tk-devel openssl-devel \
                   wget curl git

sudo dnf install cpp gcc openssl-devel \
  --setopt=timeout=600 \
  --setopt=minrate=10 \
  --setopt=retries=10
